#include <string.h>
#include <stdio.h>
int main()
{
    char str1[100],str2[100];
    int i,n,count=0;
    char m;
    scanf("%[^\n]s",str1);
    scanf("%s",str2);
    n=strlen(str1);
    m=str2[strlen(str2)-1];
    for(i=0;i<n;i++)
    {
        if(str1[i]==m)
        {
            count++;
        }
    }
    printf("%d",count);
}
